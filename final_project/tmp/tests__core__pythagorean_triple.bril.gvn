@main(n: int) {
  one.1: int = const 1;
  n_sq.1: int = mul n n;
  a.3: int = id one.1;
.outer_loop:
  b.3: int = id one.1;
.inner_loop:
  a_sq.1: int = mul a.3 a.3;
  b_sq.1: int = mul b.3 b.3;
  sum.1: int = add a_sq.1 b_sq.1;
  found.1: bool = eq sum.1 n_sq.1;
  br found.1 .found .inner_continue;
.found:
  print b.3 a.3;
.inner_continue:
  b.3: int = add b.3 one.1;
  done.1: bool = ge b.3 a.3;
  br done.1 .outer_continue .inner_loop;
.outer_continue:
  a.3: int = add a.3 one.1;
  done.2: bool = ge a.3 n;
  br done.2 .finish .outer_loop;
.finish:
}
