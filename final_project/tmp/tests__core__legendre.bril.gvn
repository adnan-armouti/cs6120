@main(n: int, p: int) {
  v18.1: int = const 0;
  i.2: int = const 1;
.loop:
  p_to_exp.1: int = call @naivePower p i.2;
  v12.1: int = div n p_to_exp.1;
  v14.1: int = const 0;
  v15.1: bool = eq v12.1 v14.1;
  br v15.1 .end .body;
.body:
