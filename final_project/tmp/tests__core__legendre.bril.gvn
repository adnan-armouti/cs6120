@naivePower(base: int, exponent: int): int {
  v2.1: int = const 0;
  v3.1: bool = eq exponent v2.1;
  br v3.1 .then.0 .else.0;
.then.0:
  v4.1: int = const 1;
  ret v4.1;
.else.0:
  v8.1: int = const 1;
  v9.1: int = sub exponent v8.1;
  v10.1: int = call @naivePower base v9.1;
  v11.1: int = mul base v10.1;
  ret v11.1;
}
