@dot_product(vectorA: ptr<int>, vectorB: ptr<int>, size: int): int {
  one.1: int = const 1;
  index.3: int = const 0;
  answer.2: int = id index.3;
.loop:
  ptrA.1: ptr<int> = ptradd vectorA index.3;
  ptrB.1: ptr<int> = ptradd vectorB index.3;
  valA.1: int = load ptrA.1;
  valB.1: int = load ptrB.1;
  tmp.1: int = mul valA.1 valB.1;
  answer.3: int = add answer.2 tmp.1;
  index.3: int = add index.3 one.1;
  cond.1: bool = lt index.3 size;
  answer.2: int = id answer.3;
  br cond.1 .loop .done;
.done:
  ret answer.3;
}
