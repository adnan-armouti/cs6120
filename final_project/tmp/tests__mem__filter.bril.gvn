  store emp_ids_ptr.1 emp_id_2.1;
  store dept_ids_ptr.1 dept_id_2.1;
  emp_id_3.1: int = const 103;
  dept_id_3.1: int = const 2;
  emp_ids_ptr_2.1: ptr<int> = ptradd emp_ids.1 two.1;
  dept_ids_ptr_2.1: ptr<int> = ptradd dept_ids.1 two.1;
  store emp_ids_ptr_2.1 emp_id_3.1;
  store dept_ids_ptr_2.1 dept_id_3.1;
  emp_id_4.1: int = const 104;
  dept_id_4.1: int = const 1;
  emp_ids_ptr_3.1: ptr<int> = ptradd emp_ids.1 three.1;
  dept_ids_ptr_3.1: ptr<int> = ptradd dept_ids.1 three.1;
  store emp_ids_ptr_3.1 emp_id_4.1;
  store dept_ids_ptr_3.1 dept_id_4.1;
  emp_id_5.1: int = const 105;
  dept_id_5.1: int = const 2;
  emp_ids_ptr_4.1: ptr<int> = ptradd emp_ids.1 four.1;
  dept_ids_ptr_4.1: ptr<int> = ptradd dept_ids.1 four.1;
  store emp_ids_ptr_4.1 emp_id_5.1;
  store dept_ids_ptr_4.1 dept_id_5.1;
  target_dept.1: int = const 2;
  i.2: int = const 0;
.loop:
  done.1: bool = ge i.2 table_size.1;
  br done.1 .cleanup .body;
