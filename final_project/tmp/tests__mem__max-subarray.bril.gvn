@pack(size: int, n1: int, n2: int, n3: int, n4: int, n5: int, n6: int, n7: int, n8: int, n9: int, n10: int): ptr<int> {
  one.1: int = const 1;
  i.1: int = const 0;
  array.1: ptr<int> = alloc size;
  loc.1: ptr<int> = ptradd array.1 i.1;
  store loc.1 n1;
  i.2: int = add i.1 one.1;
  loc.2: ptr<int> = ptradd array.1 i.2;
  store loc.2 n2;
  i.3: int = add i.2 one.1;
  loc.3: ptr<int> = ptradd array.1 i.3;
  store loc.3 n3;
  i.4: int = add i.3 one.1;
  loc.4: ptr<int> = ptradd array.1 i.4;
  store loc.4 n4;
  i.5: int = add i.4 one.1;
  loc.5: ptr<int> = ptradd array.1 i.5;
  store loc.5 n5;
  i.6: int = add i.5 one.1;
  loc.6: ptr<int> = ptradd array.1 i.6;
  store loc.6 n6;
  i.7: int = add i.6 one.1;
  loc.7: ptr<int> = ptradd array.1 i.7;
  store loc.7 n7;
  i.8: int = add i.7 one.1;
  loc.8: ptr<int> = ptradd array.1 i.8;
  store loc.8 n8;
  i.9: int = add i.8 one.1;
  loc.9: ptr<int> = ptradd array.1 i.9;
  store loc.9 n9;
  i.10: int = add i.9 one.1;
  loc.10: ptr<int> = ptradd array.1 i.10;
  store loc.10 n10;
  ret array.1;
}
@max(n: int, m: int): int {
  condi.1: bool = lt n m;
  br condi.1 .less .biggereq;
.less:
  ret m;
.biggereq:
  ret n;
}
@main(size: int, n1: int, n2: int, n3: int, n4: int, n5: int, n6: int, n7: int, n8: int, n9: int, n10: int) {
  array.1: ptr<int> = call @pack size n1 n2 n3 n4 n5 n6 n7 n8 n9 n10;
  sum.1: int = const 0;
  one.1: int = const 1;
  i.2: int = id sum.1;
  max_sum.3: int = id sum.1;
  sum.3: int = id sum.1;
.loop:
  condi.1: bool = lt i.2 size;
  br condi.1 .bodyi .done;
.bodyi:
  loc.1: ptr<int> = ptradd array.1 i.2;
  elem.1: int = load loc.1;
  sum_incr.1: int = add sum.3 elem.1;
  sum.3: int = call @max sum.1 sum_incr.1;
  max_sum.3: int = call @max max_sum.3 sum.3;
  i.2: int = add i.2 one.1;
  jmp .loop;
.done:
  print max_sum.3;
  free array.1;
}
