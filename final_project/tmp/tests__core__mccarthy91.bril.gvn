@mccarthy91(n: int): int {
  ten.1: int = const 10;
  eleven.1: int = const 11;
  hundred.1: int = const 100;
  big.1: bool = gt n hundred.1;
  br big.1 .big .small;
.big:
  tmp.1: int = sub n ten.1;
  ret tmp.1;
.small:
  tmp.2: int = add n eleven.1;
  tmp.3: int = call @mccarthy91 tmp.2;
  tmp.4: int = call @mccarthy91 tmp.3;
  ret tmp.4;
}
@main(n: int) {
  result.1: int = call @mccarthy91 n;
  print result.1;
}
