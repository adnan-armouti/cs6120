@main(in: int) {
  ten.1: int = const 10;
  zero.1: int = const 0;
  one.1: int = const 1;
  not_finished.2: bool = const true;
  index.2: int = id one.1;
.for.cond:
  br not_finished.2 .for.body .for.end;
.for.body:
  power.1: int = call @pow ten.1 index.2;
  d.1: int = div in power.1;
  check.1: bool = eq d.1 zero.1;
  br check.1 .if.true .if.false;
.if.true:
  not_finished.2: bool = const false;
  jmp .for.cond;
.if.false:
  index.2: int = add index.2 one.1;
  jmp .for.cond;
.for.end:
  exp.1: int = sub index.2 one.1;
  is_palindrome.1: bool = call @palindrome in exp.1;
  print is_palindrome.1;
}
@pow(base: int, exp.1: int): int {
  res.1: int = const 1;
  zero.1: int = const 0;
  not_finished.1: bool = const true;
  res.3: int = id res.1;
.for.cond.pow:
  br not_finished.1 .for.body.pow .for.end.pow;
.for.body.pow:
  finished.1: bool = eq exp.1 zero.1;
  br finished.1 .if.true.pow .if.false.pow;
.if.true.pow:
  not_finished.1: bool = const false;
  jmp .for.cond.pow;
.if.false.pow:
  res.3: int = mul res.3 base;
  exp.1: int = sub exp.1 res.1;
  jmp .for.cond.pow;
.for.end.pow:
  ret res.3;
}
@palindrome(in: int, len: int): bool {
  zero.1: int = const 0;
  two.1: int = const 2;
  ten.1: int = const 10;
  check.1: bool = le len zero.1;
  br check.1 .if.true.palindrome .if.false.palindrome;
.if.true.palindrome:
  is_palindrome.2: bool = const true;
  jmp .if.end.palindrome;
.if.false.palindrome:
  power.1: int = call @pow ten.1 len;
  left.1: int = div in power.1;
  v1.1: int = div in ten.1;
  v2.1: int = mul v1.1 ten.1;
  right.1: int = sub in v2.1;
  is_equal.1: bool = eq left.1 right.1;
  br is_equal.1 .if.true.mirror .if.false.mirror;
.if.true.mirror:
  temp.1: int = mul power.1 left.1;
  temp.2: int = sub in temp.1;
  temp.3: int = sub temp.2 right.1;
  next_in.1: int = div temp.3 ten.1;
  next_len.1: int = sub len two.1;
  is_palindrome.2: bool = call @palindrome next_in.1 next_len.1;
  jmp .if.end.palindrome;
.if.false.mirror:
  is_palindrome.2: bool = const false;
  jmp .if.end.palindrome;
.if.end.palindrome:
  ret is_palindrome.2;
}
